{
    "appenders": {
        "stdout": {
            "type":"console"
        },
        "app": {
            "type": "dateFile",
            "filename": "logs/app",
            "pattern": "yyyyMMdd_hh.log",
            "alwaysIncludePattern": true
        }
    },
    "replaceConsole": true,
    "categories": {
        "default": {"appenders":["stdout", "app"], "level":"ALL"}
    }
}
